{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Current ts spec file",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/node_modules/protractor/bin/protractor",
            "args": ["${workspaceFolder}/protractor.conf.ts","--specs=${relativeFile}"],
            "cwd": "${workspaceFolder}",
            "protocol": "inspector",
            "internalConsoleOptions": "openOnSessionStart",
            "outFiles": [
                "./dist"
            ]
        }
    ],
    "compounds": []
}